<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daemonw.file">

    <application>
        <activity android:name="com.daemonw.file.ui.activity.FileActivity" />
        <activity android:name="com.daemonw.file.ui.activity.GrantPermissionActivity" />
        <activity
            android:name="com.daemonw.file.ui.activity.FileChooseActivity"
            android:theme="@style/FileChooseDialogTheme" />

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="com.daemonw.file.fileProvider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>
</manifest>
